<UserControl x:Class="AXNode.SubSystem.NodeLibSystem.NodeLibPanel"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns="https://github.com/avaloniaui"
             mc:Ignorable="d"
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Background="#181818">
        <StackPanel Orientation="Horizontal" VerticalAlignment="Top" Height="30" Background="#262626" IsVisible="False">
            <Button x:Name="Tool_ExpandAll"  Width="30" Height="30" IsEnabled="False">
                <Button.Content>
                    <Image Source="../../Assets/Icon16/ExpandAll.png" Height="16" Width="16"/>
                </Button.Content>
            </Button>
            <Button x:Name="Tool_FurlAll"  Width="30" Height="30" IsEnabled="False">
                <Button.Content>
                    <Image Source="../../Assets/Icon16/FurlAll.png" Height="16" Width="16"/>
                </Button.Content>
            </Button>
        </StackPanel>
        <!-- <xc:TreeView x:Name="NodePresetTree" MoveItem="False" MultiSelect="False" DragFileOnly="True" Margin="0,0,0,0"/> -->
    </Grid>
</UserControl>